      			<material name="">
        			<color rgba="0.2 0.2 0.2 1" />
      			</material>
    		</visual>
    		<collision>
      			<origin rpy="0 0 0" xyz="0 0 0" />
      			<geometry>
        			<mesh filename="package://robotican_description/lizi/meshes/RR_Wheel_Link.STL" />
      			</geometry>
    		</collision>
  	</link>

	<!--         END         -->
	<!-- Load all the joints -->
	<joint name="FL_Wheel_Joint" type="$(arg wheels_type)">
    		<origin xyz="0.118 0.1338 0.026" rpy="0 0 -1.3371E-16" />
    		<parent link="base_link" />
    		<child link="FL_Wheel_link" />
    		<axis xyz="0 1 0" />
  	</joint>
	<joint name="RL_Wheel_Joint" type="$(arg wheels_type)">
    		<origin xyz="-0.118 0.1338 0.026" rpy="0 0 -6.6853E-16" />
    		<parent link="base_link" />
    		<child link="RL_Wheel_link" />
    		<axis xyz="0 1 0" />
    		<limit effort="0" velocity="0" />
  	</joint>
	<joint name="RR_Wheel_Joint" type="$(arg wheels_type)">
    		<origin xyz="-0.118 -0.1338 0.026" rpy="-1.2938E-16 -1.2938E-15 -6.6853E-16" />
    		<parent link="base_link" />
    		<child link="RR_Wheel_link" />
    		<axis xyz="0 1 0" />
  	</joint>
	<joint name="FR_Wheel_Joint" type="$(arg wheels_type)">
    		<origin xyz="0.118 -0.1338 0.026" rpy="4.4216E-16 -0.0061583 -2.6824E-16" />
    		<parent link="base_link" />
    		<child link="FR_Wheel_link" />
    		<axis xyz="0 1 0" />
  	</joint>	

	<!--         END         -->
</robot>
